 <!-- **************************** JOINTS ***************************** -->
  <joint name="base_fixed" type="fixed">
    <parent link="base"/>
    <child link="base_motor"/>
  </joint>


  <joint name="joint1" type="revolute">
    <parent link="base_motor"/>
    <child link="link1_motor"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <limit effort="100" velocity="100" lower="-3.14" upper="3.14" />
    <axis xyz="0 0 1" />
  </joint>

  <joint name="joint1_motor_link" type="fixed">
    <parent link="link1_motor"/>
    <child link="link1_link"/>
  </joint>
  
  <joint name="joint1_link_tapa" type="fixed">
    <parent link="link1_link"/>
    <child link="link1_tapa"/>
  </joint>

  <joint name="joint1_tapa_nbio" type="fixed">
    <parent link="link1_tapa"/>
